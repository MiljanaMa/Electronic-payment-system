{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "panels": [
    {
      "type": "graph",
      "title": "Webshop: Total Requests",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(http_server_requests_seconds_count{application=\"Webshop\"})",
          "legendFormat": "Webshop Requests",
          "interval": ""
        }
      ],
      "gridPos": {"x": 0, "y": 0, "w": 12, "h": 6}
    },
    {
      "type": "graph",
      "title": "Webshop: Requests per Second",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(http_server_requests_seconds_count{application=\"Webshop\"}[1m])",
          "legendFormat": "Webshop RPS",
          "interval": ""
        }
      ],
      "gridPos": {"x": 12, "y": 0, "w": 12, "h": 6}
    },
    {
      "type": "graph",
      "title": "Webshop: Average Response Time (ms)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "(rate(http_server_requests_seconds_sum{application=\"Webshop\"}[1m]) / rate(http_server_requests_seconds_count{application=\"Webshop\"}[1m])) * 1000",
          "legendFormat": "Avg Response Time",
          "interval": ""
        }
      ],
      "gridPos": {"x": 0, "y": 6, "w": 12, "h": 6}
    },
    {
      "type": "graph",
      "title": "PSP: Total Requests",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(http_server_requests_seconds_count{application=\"PSP\"})",
          "legendFormat": "PSP Requests",
          "interval": ""
        }
      ],
      "gridPos": {"x": 12, "y": 6, "w": 12, "h": 6}
    },
    {
      "type": "graph",
      "title": "PSP: Requests per Second",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(http_server_requests_seconds_count{application=\"PSP\"}[1m])",
          "legendFormat": "PSP RPS",
          "interval": ""
        }
      ],
      "gridPos": {"x": 0, "y": 12, "w": 12, "h": 6}
    },
    {
      "type": "graph",
      "title": "PSP: Average Response Time (ms)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "(rate(http_server_requests_seconds_sum{application=\"PSP\"}[1m]) / rate(http_server_requests_seconds_count{application=\"PSP\"}[1m])) * 1000",
          "legendFormat": "Avg Response Time",
          "interval": ""
        }
      ],
      "gridPos": {"x": 12, "y": 12, "w": 12, "h": 6}
    }
  ],
  "schemaVersion": 37,
  "title": "Webshop & PSP Metrics",
  "version": 1
}
